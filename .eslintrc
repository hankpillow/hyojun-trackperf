{
    "rules": {
      "no-console": 0,
      "no-empty" : 0,
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "browser": true
    },
    "globals" : {
      "console" : true
    },
    "extends": "eslint:recommended"
}
